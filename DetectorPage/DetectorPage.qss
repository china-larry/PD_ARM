}

QTableWidget
{
    border: 1px solid #0068b7;
    border-radius: 3px;
    selection-background-color: #60c8ff;
}
QHeaderView::section
{
    background-color: #161F30;
    height:35px;
    color: #e2e4ed;
    font-size: 15px;
    font-family:"Arial";
 /*   font-weight:bold;*/
}
QPushButton
{
    font-size: 16px;
    font-family:"Arial";
    font-weight:bold;
    border: 0px solid #0068b7;
    border-radius: 5px;
    color: white;
    background-color: rgb(30, 129, 255);
}
QPushButton:disabled {
    background-color:#D5D5D5;
    color: rgb(60, 60, 60);
}
QPushButton:enabled {
    background-color: rgb(30, 129, 255);
}

QPushButton:hover{
        background: rgb(85, 85, 85);
}

QPushButton:pressed{
        background: rgb(80, 80, 80);
}
QRadioButton
{
    color: #333; /* text color:/image/ico/setting/set_language_checked.png */
    font-family: "Arial";
    font-size: 16px;
}
QRadioButton::indicator {
    width: 21px;
    height: 21px;
}
/*未选中*/
QRadioButton::indicator::unchecked {
    image: url(:/image/ico/setting/set_language_unchecked.png);
}
/*选中*/
QRadioButton::indicator::checked {
    image: url(:/image/ico/setting/set_language_checked.png);
}
